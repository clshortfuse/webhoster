    "./lib/*": "./lib/*",
    "./middlewares/*": "./middlewares/*",
    "./polyfills/*": "./polyfills/*",
    "./templates/*": "./templates/*",
    "./types/*": "./types/*",
    "./utils/*": "./utils/*"
